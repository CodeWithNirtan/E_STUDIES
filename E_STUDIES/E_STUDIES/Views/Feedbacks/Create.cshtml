<div class="contact1">
    <div class="container-contact1">
        <div class="contact1-pic js-tilt" data-tilt>
            <img src="~/images/img-01.png" alt="IMG">
        </div>

        @*<form class="contact1-form validate-form">*@
        @using (Html.BeginForm())
        {
            <div class="contact1-form validate-form">
                @Html.AntiForgeryToken()
                <span class="contact1-form-title">
                    Feedback
                </span>
                <div class="floating-label mt-5">
                    @*<input placeholder="Subject" type="Subject"
                        autocomplete="off">*@

                    @Html.EditorFor(model => model.Subjects, new { htmlAttributes = new { name = "Subject", id = "Subject", placeholder = "Subject", autocomplete = "off" } })
                    @Html.ValidationMessageFor(model => model.Subjects, "", new { @class = "text-danger" })
                    <label for="Subject">Subject:</label>
                    <div class="icon">
                        <?xml version="1.0" encoding="UTF-8" ?>
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                            <path d="M15.71,12.71a6,6,0,1,0-7.42,0,10,10,0,0,0-6.22,8.18,1,1,0,0,0,2,.22,8,8,0,0,1,15.9,0,1,1,0,0,0,1,.89h.11a1,1,0,0,0,.88-1.1A10,10,0,0,0,15.71,12.71ZM12,12a4,4,0,1,1,4-4A4,4,0,0,1,12,12Z" />
                        </svg>

                    </div>
                </div>
                <div class="floating-label mt-5">
                    @*<textarea style="overflow:hidden" placeholder="Message" name="Message" id="Message" autocomplete="off"></textarea>*@
                    @Html.EditorFor(model => model.Feedback_messages, new { htmlAttributes = new { placeholder = "Message", name = "Message", id = "Message", autocomplete = "off" } })
                    @Html.ValidationMessageFor(model => model.Feedback_messages, "", new { @class = "text-danger" })


                    <label for="Message">Message:</label>
                    <div class="icon">
                        <?xml version="1.0" encoding="UTF-8" ?>
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                            <path d="M15.71,12.71a6,6,0,1,0-7.42,0,10,10,0,0,0-6.22,8.18,1,1,0,0,0,2,.22,8,8,0,0,1,15.9,0,1,1,0,0,0,1,.89h.11a1,1,0,0,0,.88-1.1A10,10,0,0,0,15.71,12.71ZM12,12a4,4,0,1,1,4-4A4,4,0,0,1,12,12Z" />
                        </svg>

                    </div>
                </div>
                <div class="container-contact1-form-btn mt-5">
                    <button class="contact1-form-btn" type="submit" value="create">
                        <span>
                            Send Feedback
                            <i class="fa fa-long-arrow-right" aria-hidden="true"></i>
                        </span>
                    </button>
                </div>
            </div>

  
    }
        @*</form>*@


    </div>
    </div>




